TEMP_IR_PATH=$MERLIN_ROOT/Bin/mono_debug
IR_OPTIONS=-X:Interpret

export RUBY_EXE=$TEMP_IR_PATH/ir.exe

mono $TEMP_IR_PATH/ir.exe $IR_OPTIONS $*
